{
	"name": "${mod_name}",
	"id": "${mod_id}",
	"version": "${version}",
	"icon": "assets/${mod_id}/textures/logo.png",
	"description": "More building blocks!",
	"authors": [
		"Hugman",
		"Dawn Team"
	],
	"contact": {
		"homepage": "https://www.curseforge.com/minecraft/mc-mods/${curseforge_slug}",
		"sources": "https://github.com/DawnTeamMC/${github_name}",
		"issues": "https://github.com/DawnTeamMC/${github_name}/issues"
	},
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.hugman.nookbuild.NookBuild"
		]
	},
	"depends": {
		"minecraft": "*",
		"fabric": "*",
		"dawn": "*"
	},
	"custom": {
		"modupdater": {
			"strategy": "curseforge",
			"projectID": "${curseforge_id}"
		}
	},
	"license": "MIT",
	"schemaVersion": 1
}
